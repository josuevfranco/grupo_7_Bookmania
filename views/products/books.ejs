    <style>
        @import url(https://fonts.googleapis.com/css?family=Neuton:300,400,700,400italic);

        figure.thumb {
            margin: 0;
            display: inline-block;
            vertical-align: top;
        }

        .thumb {
            padding: 15px;
        }

        .thumb h2,
        .thumb p {
            margin: 0;
        }

        .thumb:hover {
            transform: translateY(-10px);
        }

        .thumb h2 {
            font-weight: 700;
            font-size: 18px;
            color: rgba(0, 0, 0, .75);
            margin-top: 20px;
        }

        .thumb .author {
            font-size: 14px;
            margin-top: 5px;
            color: rgba(0, 0, 0, .55);
            font-weight: 400;
        }

        .cover {
            display: inline-block;
            position: relative;
            box-shadow: 0 10px 20px -8px;
            margin-right: 20px;
        }

        .cover:after {
            content: '';
            overflow: hidden;
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0;
        }


        .cover img {
            height: 200px;
            display: block;
            position: relative;
        }

        /********************************************************************************************************/
        @import url('https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700;900&display=swap');

        .maindiv {
            margin: 4rem;
            background: white;
            border-radius: 1rem;
            box-shadow: 2px 5px 20px #2323235e;
        }

        .innerdiv h2 {
            margin: 0.7rem 0 0.4rem;
            color: #03A9F4;
        }

        .innerdiv h1 img {
            height: 2rem;
            margin-right: 0.5rem;
        }

        .innerdiv p {
            font-size: 1.2rem;
            color: #969393;
            margin-bottom: 1rem;

        }

        .eachdiv {
            display: inline-block;
            width: 23.5%;
            background: white;
            box-shadow: 1px 2px 10px #33333352;
            padding: 1rem;
            margin: 0.5rem 0.4%;
            border-radius: 1rem;
            cursor: pointer;
            transition: 0.4s;
        }


        .eachdiv:hover {
            transform: scale(1.03);
            box-shadow: 2px 6px 10px #2f2f2f69;
        }

        .eachdiv img {
            width: 100%;
            margin-bottom: 0.8rem;
            border-radius: 1rem;
        }

        .eachdiv h3 {
            color: #E91E63;
            margin-bottom: 0.5rem;
        }

        .eachdiv p {
            font-size: 0.8rem;
            color: #969696;
            margin-bottom: 0;
        }

        
        @media only screen and (max-width: 1000px) {
            .maindiv {
                margin: 3rem;
            }

            .eachdiv {
                width: 48%;
            }
        }

        @media only screen and (max-width: 600px) {
            .maindiv {
                margin: 2rem;
            }

            .eachdiv {
                width: 100%;
                margin: 0.5rem 0;
            }
        }

        .price {
            color: rgb(65, 80, 151);
        }
    </style>
</head>

